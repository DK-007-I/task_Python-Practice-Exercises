class Hotel:
    """–ö–ª–∞—Å—Å –¥–ª—è –±—Ä–æ–Ω–∏—Ä–æ–≤–∞–Ω–∏—è –∏ —Å–Ω—è—Ç–∏—è –±—Ä–æ–Ω–∏ –Ω–æ–º–µ—Ä–æ–≤ –æ—Ç–µ–ª—è üè†"""

    def __init__(self, name):
        self.name = name
        self.rooms = {
            100: True, 101: True, 102: True, 103: True,
            200: True, 201: True, 202: True, 203: True
        }

    def print_rooms(self):
        print(self.name)
        for room, is_free in self.rooms.items():
            status = "—Å–≤–æ–±–æ–¥–µ–Ω" if is_free else "–∑–∞–Ω—è—Ç"
            print(f"–ù–æ–º–µ—Ä ‚Ññ {room} {status}")

    def book_room_by_number(self, room_number):
        if room_number not in self.rooms:
            print("–û—à–∏–±–∫–∞! –¢–∞–∫–æ–≥–æ –Ω–æ–º–µ—Ä–∞ –Ω–µ—Ç.")
        elif not self.rooms[room_number]:
            print(f"–ù–æ–º–µ—Ä {room_number} —É–∂–µ –∑–∞–±—Ä–æ–Ω–∏—Ä–æ–≤–∞–Ω.")
        else:
            self.rooms[room_number] = False

    def finish_number_by_number(self, room_number):
        if room_number not in self.rooms:
            print("–û—à–∏–±–∫–∞! –¢–∞–∫–æ–≥–æ –Ω–æ–º–µ—Ä–∞ –Ω–µ—Ç.")
        elif self.rooms[room_number]:
            print(f"–ù–æ–º–µ—Ä {room_number} –Ω–µ –∑–∞–±—Ä–æ–Ω–∏—Ä–æ–≤–∞–Ω.")
        else:
            self.rooms[room_number] = True


# –ú–µ–Ω—é
hotel = Hotel("–û—Ç–µ–ª—å: –£ –û–ª–µ–≥–∞")
while True:
    print("\n[1] - –í—ã–≤–µ—Å—Ç–∏ –∑–∞–Ω—è—Ç–æ—Å—Ç—å –≤—Å–µ—Ö –Ω–æ–º–µ—Ä–æ–≤")
    print("[2] - –ó–∞–Ω—è—Ç—å –Ω–æ–º–µ—Ä")
    print("[3] - –°–Ω—è—Ç—å –±—Ä–æ–Ω—å —Å –Ω–æ–º–µ—Ä–∞")
    print("[4] - –í—ã—Ö–æ–¥")
    choice = input("> ")

    if choice == '1':
        hotel.print_rooms()
    elif choice == '2':
        try:
            number = int(input("–í–≤–µ–¥–∏—Ç–µ –Ω–æ–º–µ—Ä –∫–æ–º–Ω–∞—Ç—ã –¥–ª—è –±—Ä–æ–Ω–∏.\n> "))
            hotel.book_room_by_number(number)
        except ValueError:
            print("–û—à–∏–±–∫–∞. –í–≤–µ–¥–∏—Ç–µ –Ω–æ–º–µ—Ä –∫–æ–º–Ω–∞—Ç—ã.")
    elif choice == '3':
        try:
            number = int(input("–í–≤–µ–¥–∏—Ç–µ –Ω–æ–º–µ—Ä –∫–æ–º–Ω–∞—Ç—ã –¥–ª—è –æ—Ç–º–µ–Ω—ã –±—Ä–æ–Ω–∏.\n> "))
            hotel.finish_number_by_number(number)
        except ValueError:
            print("–û—à–∏–±–∫–∞. –í–≤–µ–¥–∏—Ç–µ –Ω–æ–º–µ—Ä –∫–æ–º–Ω–∞—Ç—ã.")
    elif choice == '4':
        print("–í—ã—Ö–æ–¥.")
        break
    else:
        print("–¢–∞–∫–æ–≥–æ –≤–∞—Ä–∏–∞–Ω—Ç–∞ –Ω–µ—Ç.")
